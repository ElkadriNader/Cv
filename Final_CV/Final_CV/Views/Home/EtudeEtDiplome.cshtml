    <script src="~/Templates/js/demo/peity-demo.js"></script>

    <!-- Custom and plugin javascript -->
    <script src="~/Templates/js/inspinia.js"></script>
    <script src="~/Templates/js/plugins/pace/pace.min.js"></script>

